{
  "version": 2,
  "builds": [
    {
      "src": "apps/backend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "apps/backend/api/test.js"
    },
    {
      "src": "/debug",
      "dest": "apps/backend/api/debug.js"
    },
    {
      "src": "/(.*)",
      "dest": "apps/backend/api/index.js"
    }
  ]
}
