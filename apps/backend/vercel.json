{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "api/test.js"
    },
    {
      "src": "/debug",
      "dest": "api/debug.js"
    },
    {
      "src": "/body-debug",
      "dest": "api/body-debug.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
